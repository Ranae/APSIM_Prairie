    <component name="unit convertor" executable="%apsim%\Model\Manager.dll" class="Manager">
      <executable name="%apsim%\Model\Manager.dll" version="1.0" />
      <initdata>
        <script>
          <text>! sotiris
 
 
!surfaceom_cover = 0.1
 
</text>
          <event>init</event>
        </script>
        <script>
          <text>sotiris = BelowGroundWt + fom_c()/0.44
fom_cn = fom_c() / fom_n()
sotiris1 = BelowGroundWt - toRootDM
Total_m_respi = spResp_m(1) + spResp_m(2) + spResp_m(3)
Root_m_respi = ToRoot * Total_m_respi
 
 
! CN ratio calculations
if surfaceom_n &gt; 0.1 then
surface_CN_ratio = surfaceom_c / surfaceom_n
endif
 
if BelowGroundN &gt; 0.1 then
roots_cn = BelowGroundWt*0.4/BelowGroundN
endif
 
if BelowGroundWt &gt; 0.1 then
roots_N% = 100* BelowGroundN / BelowGroundWt
endif
 
! Check calculations for FOM cnrf
! FOM dec = amount * rate * tf * mf * cnrf
 
sot_cnr = fom_c() / (fom_n() + nit_tot())
sot_cnrf = exp(-0.677 * (sot_cnr - 25) / 25))
tf_surface_om1 = SurfaceOrganicMatter.tf
 
 
! SOC calculations
oc_5cm = 0.5 * (oc(1) + oc(2))
oc_15cm = 0.5 * (oc(3) + oc(4))
oc_30cm = 0.5 * (oc(5) + oc(6))
oc_60cm = 0.6 * oc(7) + 0.4*oc(8)
oc_100cm = 0.1* oc(8) + 0.9*oc(9)
 
 
! Soil and temperature calculations
sw_5 = (sw(1) + sw(2))*0.5
sw_10 = sw(3)
sw_15 = sw(4)
sw_35 = sw(6)
sw_75 = sw(7) ! this is 50 cm
!st_5 = (st(1) + st(2))*0.5
!st_10 = st(3)
!st_15 = st(4)
!st_35 = st(6)
!st_75 = st(7) ! this is 50 cm
st_5 = (ave_soil_temp(1) + ave_soil_temp(2))*0.5
st_10 = ave_soil_temp(3)
st_15 = ave_soil_temp(4)
st_35 = ave_soil_temp(5)
st_75 = ave_soil_temp(6)
 
 
! soil respitation
soil_co2 = dlt_biom_c_atm() + dlt_fom_c_atm() + dlt_hum_c_atm() + dlt_res_c_atm
! root respiration
! The growth component of the respiration is assumed to be accounted in the RUE value
! So only the maintenance component of the root respiration is modeled here following Teixeira et al., 2009 Crop &amp; Pasture Science 60: 778-784
!root_co2 = toRootDM * 0.44 * 0.015 * 2.0^((st(1)-20)/10)         ! the 0.44 is to convert dry mater to C, Q10=1.8, Tref = 20.
!root_co2 = SumNewRoot * 0.44 * 0.015 * 2.0^((st(1)-20)/10))         ! the 0.44 is to convert dry mater to C, Q10=1.8, Tref = 20.
root_co2 = Root_m_respi
 
! total soil + root respiration
soil_root_co2 = soil_co2 + root_co2
bbc_gradient = -1
 
 
!bbc_gradient = -1.0025
 
if subsurface_drain &gt;0 then
N_conc_mgLN = 100* subsurface_drain_no3 / subsurface_drain
endif
 
 
</text>
          <event>start_of_day</event>
        </script>
        <script>
          <text>
          </text>
          <event>end_of_day</event>
        </script>
      </initdata>
    </component>
